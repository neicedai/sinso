cp sinso.service /etc/systemd/system
cd /etc/systemd/system
port1=1634
port2=11634
port3=21634
for ((i=1;i<=16;i++)) do
cp sinso.service sinso$i.service
sed -i 's#A#'$port1'#g' sinso$1.service
sed -i 's#B#'$port2'#g' sinso$1.service
sed -i 's#C#'$port3'#g' sinso$1.service
sed -i 's#D#'$i'#g' sinso$1.service
port1=`expr $port1 + 1`
port2=`expr $port2 + 1`
port3=`expr $port3 + 1`
done
